{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.5",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "composer/package-versions-deprecated": "1.11.99.1",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.3",
    "doctrine/doctrine-migrations-bundle": "^3.1",
    "doctrine/orm": "2.14.2",
    "friendsofsymfony/rest-bundle": "^3.0",
    "gesdinet/jwt-refresh-token-bundle": "^0.12.0",
    "kgabryel/error-converter": "dev-master",
    "kgabryel/routing": "dev-master",
    "league/oauth2-client": "^2.7",
    "league/oauth2-facebook": "^2.0",
    "lexik/jwt-authentication-bundle": "^2.13",
    "nelmio/cors-bundle": "^2.1",
    "phpdocumentor/reflection-docblock": "^5.2",
    "ramsey/uuid": "^4.2",
    "symfony/console": "5.2.*",
    "symfony/dotenv": "5.2.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "5.2.*",
    "symfony/framework-bundle": "5.2.*",
    "symfony/http-client": "5.2.*",
    "symfony/http-foundation": "^5.4",
    "symfony/mailer": "^5.4",
    "symfony/property-access": "5.2.*",
    "symfony/property-info": "5.2.*",
    "symfony/proxy-manager-bridge": "5.2.*",
    "symfony/serializer": "5.2.*",
    "symfony/twig-bundle": "5.2",
    "symfony/validator": "5.2.*",
    "symfony/yaml": "5.2.*",
    "symfonycasts/reset-password-bundle": "^1.14"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "symfony/maker-bundle": "^1.39"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.2.*",
      "docker": false
    }
  }
}
